<p id="notice"><%= notice %></p>

<h1>Student Current Borrow Lists</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Book</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_current_borrow_lists.each do |student_current_borrow_list| %>
      <tr>
        <td><%= student_current_borrow_list.student_id %></td>
        <td><%= student_current_borrow_list.book_id %></td>
        <td><%= link_to 'Show', student_current_borrow_list %></td>
        <td><%= link_to 'Edit', edit_student_current_borrow_list_path(student_current_borrow_list) %></td>
        <td><%= link_to 'Destroy', student_current_borrow_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student Current Borrow List', new_student_current_borrow_list_path %>
