<p id="notice"><%= notice %></p>

<h1>Library Book Lists</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>Book</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @library_book_lists.each do |library_book_list| %>
      <tr>
        <td><%= library_book_list.number %></td>
        <td><%= library_book_list.book_id %></td>
        <td><%= link_to 'Show', library_book_list %></td>
        <td><%= link_to 'Edit', edit_library_book_list_path(library_book_list) %></td>
        <td><%= link_to 'Destroy', library_book_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Library Book List', new_library_book_list_path %>
