<h1>Books</h1>

<table>
  <thead>
  <tr>
    <th>Isbn</th>
    <th>Title</th>
    <th>University</th>
    <th>Library</th>
    <th>Is special collection</th>
    <th>Available number</th>
    <th>Hold request number</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @wish_lists.each do |w| %>
    <tr>
      <td><%= Book.find(LibraryBookList.find(w.library_book_list_id).book_id).isbn %></td>
      <td><%= Book.find(LibraryBookList.find(w.library_book_list_id).book_id).title %></td>
      <td><%= University.find(Book.find(LibraryBookList.find(w.library_book_list_id).book_id).university_id).name %></td>
      <td><%= Library.find(Book.find(LibraryBookList.find(w.library_book_list_id).book_id).library_id).name %></td>
      <td><%= Book.find(LibraryBookList.find(w.library_book_list_id).book_id).is_special_collection %></td>
      <td><%= LibraryBookList.find(w.library_book_list_id).number %></td>
      <td><%= LibraryBookList.find(w.library_book_list_id).hold %></td>
      <td><td><%= link_to 'delete', s_wish_list_path + "/" + w.id.to_s, method: :delete %></td></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>